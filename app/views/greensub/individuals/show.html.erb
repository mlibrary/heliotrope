<p id="notice"><%= notice %></p>
<div id="maincontent">
  <h1>Individual</h1>
  <div class="col-md-1"><em>ID</em></div>
  <div class="col-md-11">&nbsp<%= @individual.id %></div>
  <div class="col-md-1"><em>Identifier</em></div>
  <div class="col-md-11">&nbsp<%= @individual.identifier %></div>
  <div class="col-md-1"><em>Name</em></div>
  <div class="col-md-11">&nbsp<%= @individual.name %></div>
  <div class="col-md-1"><em>eMail</em></div>
  <div class="col-md-11">&nbsp<%= @individual.email %></div>
  <div class="col-md-1"><em>Lessee</em></div>
  <div class="col-md-11">&nbsp<%= link_to @individual.lessee.id, greensub_lessee_path(@individual.lessee) %></div>
  <div class="col-md-12"><br/></div>
  <div class="col-md-12">
    <%= link_to_if @individual.update?, 'Edit', edit_greensub_individual_path(@individual) %>
    <span> | </span>
    <%= link_to 'Back', greensub_individuals_path %>
  </div>
  <div class="col-md-12"><br/></div>
</div>
