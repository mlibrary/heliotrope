<video id="video" preload="auto" data-able-player data-include-transcript="false">
  <source src="<%= hyrax.download_path(file_set, file: 'webm') %>" type="video/webm" />
  <source src="<%= hyrax.download_path(file_set, file: 'mp4') %>" type="video/mp4" />
  <% if @presenter.transcript.present? %>
    <track kind="captions" src="<%= hyrax.download_path(file_set, file: 'vtt') %>" srclang="en" label="English"  />
  <% end %>
  Your browser does not support the video tag.
</video>
