<% # container for document in index list view -%>
<% presenter = Hyrax::MonographPresenter.new(document, current_ability) %>
<% counter = document_counter_with_offset(document_counter) %>
<% marker = presenter.open_access? ? "oa-monograph" : "monograph" %>
<%= render partial: "index_list", locals: { document: document, counter: counter, marker: marker, presenter: presenter } %>
